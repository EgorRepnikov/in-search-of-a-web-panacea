{
  "Fastify.js": {
    "dir": "node/fastify",
    "build": "npm install",
    "clean": "rm -rf node_modules && rm package-lock.json",
    "run": {
      "command": "node",
      "args": ["index.js"]
    },
    "supports": [
      "proxy"
    ]
  },
  "Actix": {
    "dir": "rust/actix",
    "build": "cargo build --release",
    "clean": "cargo clean",
    "run": {
      "command": "./target/release/actix",
      "args": []
    },
    "supports": [
      "proxy"
    ]
  }
}
